<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.dddml.wms.domain.documenttype">
  
  <class name="org.dddml.wms.domain.documenttype.AbstractDocumentTypeState$SimpleDocumentTypeState" table="DocumentTypes">

	<id name="documentTypeId" length="50" column="DocumentTypeId"><generator class="assigned"/></id>
	<version name="version" column="Version" type="long" />

	<property name="description" ><column name="Description" not-null="true" /></property>
	<property name="parentDocumentTypeId" ></property>
	<property name="createdBy" ></property>
	<property name="updatedBy" ></property>
	<property name="active" ></property>
	<property name="deleted" ></property>

    <property name="createdAt" column="CreatedAt" />

    <property name="updatedAt" column="UpdatedAt" />

    <property name="commandId" column="CommandId"/>



  </class>
  
</hibernate-mapping>

