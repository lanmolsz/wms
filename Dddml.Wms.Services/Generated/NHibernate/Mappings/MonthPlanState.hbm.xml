<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
				   assembly="Dddml.Wms.Common" 
				   namespace="Dddml.Wms.Domain">
  
  <class name="Dddml.Wms.Domain.MonthPlanState" table="MonthPlans">

	<composite-id name="MonthPlanId" class="Dddml.Wms.Domain.MonthPlanId">
		<key-property name="PersonalNameFirstName" column="MonthPlanIdPersonalNameFirstName" length="50"/>
		<key-property name="PersonalNameLastName" column="MonthPlanIdPersonalNameLastName" length="50"/>
		<key-property name="Year" column="MonthPlanIdYear" />
		<key-property name="Month" column="MonthPlanIdMonth" />
	</composite-id>

	<version name="Version" type="long" unsaved-value="0"/>

	<property name="Description" length="500"></property>

	<property name="CreatedBy" ></property>

	<property name="CreatedAt" ></property>

	<property name="UpdatedBy" ></property>

	<property name="UpdatedAt" ></property>

	<property name="Active" ></property>

	<property name="Deleted" ></property>


  </class>
  
</hibernate-mapping>

