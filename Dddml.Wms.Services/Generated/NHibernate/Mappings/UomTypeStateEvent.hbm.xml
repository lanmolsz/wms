<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="Dddml.Wms.Common" 
                   namespace="Dddml.Wms.Domain.UomType">
  
  <class name="UomTypeStateEventBase" table="UomTypeStateEvents" mutable="false" abstract="true">

    <composite-id name="StateEventId" class="Dddml.Wms.Domain.UomType.UomTypeStateEventId">
        <key-property name="UomTypeId" ><column name="UomTypeId" sql-type="VARCHAR(20)" /></key-property>
        <key-property name="Version" ></key-property>
    </composite-id>
    <discriminator column="StateEventType" type="String" />

    <property name="ParentTypeId" ><column name="ParentTypeId" sql-type="VARCHAR(20)" /></property>
    <property name="HasTable" ><column name="HasTable" sql-type="CHAR(1)" /></property>
    <property name="Description" ><column name="Description" sql-type="VARCHAR(255)" /></property>
    <property name="Active" ></property>

    <property name="CreatedBy" column="CreatedBy"/>

    <property name="CreatedAt" column="CreatedAt" type="DateTimeUtc"/>
    <property name="CommandId" column="CommandId"/>
    <subclass name="UomTypeStateCreated" discriminator-value="Created">
    	
    </subclass>

    <subclass name="UomTypeStateMergePatched"  discriminator-value="MergePatched">


        <property name="IsPropertyParentTypeIdRemoved" column="IsPropertyParentTypeIdRemoved"/>

        <property name="IsPropertyHasTableRemoved" column="IsPropertyHasTableRemoved"/>

        <property name="IsPropertyDescriptionRemoved" column="IsPropertyDescriptionRemoved"/>

        <property name="IsPropertyActiveRemoved" column="IsPropertyActiveRemoved"/>
    </subclass>

    <subclass name="UomTypeStateDeleted"  discriminator-value="Deleted">

    </subclass>



  </class>
  
</hibernate-mapping>
