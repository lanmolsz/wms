<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.dddml.wms.domain">
  
  <class name="org.dddml.wms.domain.AbstractDayPlanState$SimpleDayPlanState" table="DayPlans">

	<composite-id name="dayPlanId" class="org.dddml.wms.domain.DayPlanId">
		<key-property name="personalNameFirstName" column="DayPlanIdPersonalNameFirstName" length="50"/>
		<key-property name="personalNameLastName" column="DayPlanIdPersonalNameLastName" length="50"/>
		<key-property name="year" column="DayPlanIdYear" />
		<key-property name="month" column="DayPlanIdMonth" />
		<key-property name="day" column="DayPlanIdDay" />
	</composite-id>

	<version name="version" type="long" />

	<property name="description" length="500"></property>

	<property name="createdBy" ></property>

	<property name="createdAt" ></property>

	<property name="updatedBy" ></property>

	<property name="updatedAt" ></property>

	<property name="active" ></property>

	<property name="deleted" ></property>


  </class>
  
</hibernate-mapping>

