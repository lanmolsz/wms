<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.dddml.wms.domain">
  
  <class name="org.dddml.wms.domain.AbstractUserPermissionState$SimpleUserPermissionState" table="UserPermissions">

	<composite-id name="userPermissionId" class="org.dddml.wms.domain.UserPermissionId">
		<key-property name="userId" column="UserPermissionIdUserId" length="50"/>
		<key-property name="permissionId" column="UserPermissionIdPermissionId" length="50"/>
	</composite-id>

	<version name="version" type="long" />

	<property name="createdBy" ></property>

	<property name="createdAt" ></property>

	<property name="updatedBy" ></property>

	<property name="updatedAt" ></property>

	<property name="active" ></property>

	<property name="deleted" ></property>


  </class>
  
</hibernate-mapping>

