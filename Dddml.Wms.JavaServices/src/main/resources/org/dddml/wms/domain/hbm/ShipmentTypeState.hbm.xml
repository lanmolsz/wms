<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.dddml.wms.domain.shipmenttype">
  
  <class name="org.dddml.wms.domain.shipmenttype.AbstractShipmentTypeState$SimpleShipmentTypeState" table="ShipmentTypes">

	<id name="shipmentTypeId" length="20" column="ShipmentTypeId"><generator class="assigned"/></id>
	<version name="version" column="Version" type="long" />

	<property name="description" ><column name="Description" sql-type="VARCHAR(255)" /></property>
	<property name="createdBy" ></property>
	<property name="updatedBy" ></property>
	<property name="active" ></property>

    <property name="createdAt" column="CreatedAt" />

    <property name="updatedAt" column="UpdatedAt" />

    <property name="commandId" column="CommandId"/>



  </class>
  
</hibernate-mapping>

