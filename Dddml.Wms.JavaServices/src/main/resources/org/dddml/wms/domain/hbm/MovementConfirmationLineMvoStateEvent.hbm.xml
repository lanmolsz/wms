    <property name="createdBy" column="CreatedBy"/>

    <property name="createdAt" column="CreatedAt" />

    <property name="commandId" column="CommandId"/>

    <subclass name="AbstractMovementConfirmationLineMvoStateEvent$SimpleMovementConfirmationLineMvoStateCreated" discriminator-value="Created">
    	
