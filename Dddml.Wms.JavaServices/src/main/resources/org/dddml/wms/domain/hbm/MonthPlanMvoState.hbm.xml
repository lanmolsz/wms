<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.dddml.wms.domain">
  
  <class name="org.dddml.wms.domain.AbstractMonthPlanMvoState$SimpleMonthPlanMvoState" table="MonthPlan_RV">

	<composite-id name="monthPlanId" class="org.dddml.wms.domain.MonthPlanId">
		<key-property name="personalNameFirstName" column="MonthPlanIdPersonalNameFirstName" length="50"/>
		<key-property name="personalNameLastName" column="MonthPlanIdPersonalNameLastName" length="50"/>
		<key-property name="year" column="MonthPlanIdYear" />
		<key-property name="month" column="MonthPlanIdMonth" />
	</composite-id>

	<version name="personVersion" type="long" />

	<property name="description" length="500"></property>

	<property name="createdBy" ></property>

	<property name="updatedBy" ></property>

	<property name="version" ></property>

	<property name="createdAt" ></property>

	<property name="updatedAt" ></property>

	<property name="active" ></property>

	<property name="deleted" ></property>

	<property name="yearPlanDescription" length="500"></property>

	<property name="yearPlanCreatedBy" ></property>

	<property name="yearPlanUpdatedBy" ></property>

	<property name="yearPlanVersion" ></property>

	<property name="yearPlanCreatedAt" ></property>

	<property name="yearPlanUpdatedAt" ></property>

	<property name="yearPlanActive" ></property>

	<property name="yearPlanDeleted" ></property>

	<property name="personBirthDate" ></property>

	<component name="personLoves" class="org.dddml.wms.domain.PersonalName">
		<property name="firstName" column="personLovesFirstName" length="50"/>
		<property name="lastName" column="personLovesLastName" length="50"/>
	</component>

	<component name="personEmergencyContact" class="org.dddml.wms.domain.Contact">
		<property name="personalNameFirstName" column="personEmergencyContactPersonalNameFirstName" length="50"/>
		<property name="personalNameLastName" column="personEmergencyContactPersonalNameLastName" length="50"/>
		<property name="phoneNumber" column="personEmergencyContactPhoneNumber" />
		<property name="address" column="personEmergencyContactAddress" />
	</component>

	<property name="personCreatedBy" ></property>

	<property name="personUpdatedBy" ></property>

	<property name="personCreatedAt" ></property>

	<property name="personUpdatedAt" ></property>

	<property name="personActive" ></property>

	<property name="personDeleted" ></property>


  </class>
  
</hibernate-mapping>

