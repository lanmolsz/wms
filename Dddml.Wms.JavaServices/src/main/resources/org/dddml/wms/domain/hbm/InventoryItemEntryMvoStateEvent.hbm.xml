	<composite-id name="stateEventId" class="org.dddml.wms.domain.inventoryitementrymvo.InventoryItemEntryMvoStateEventId">
		<key-property name="inventoryItemEntryIdInventoryItemIdProductId" ></key-property>
		<key-property name="inventoryItemEntryIdInventoryItemIdLocatorId" ></key-property>
		<key-property name="inventoryItemEntryIdInventoryItemIdAttributeSetInstance" ></key-property>
		<key-property name="inventoryItemEntryIdEntrySeqId" ></key-property>
		<key-property name="inventoryItemVersion" ></key-property>
	</composite-id>

    <discriminator column="StateEventType" type="string" />

	<property name="quantityOnHandVar" ></property>
	<property name="quantityReservedVar" ></property>
	<property name="quantityOccupiedVar" ></property>
	<property name="quantityVirtualVar" ></property>
	<component name="eventVO" class="org.dddml.wms.domain.inventoryitem.InventoryItemEventVO">
		<property name="inventoryItemEventTypeId" ><column name="eventVOInventoryItemEventTypeId" /></property>
		<property name="documentNumber" ><column name="eventVODocumentNumber" /></property>
	</component>
	<property name="version" ></property>
	<property name="active" ></property>
	<property name="inventoryItemQuantityOnHand" ></property>
	<property name="inventoryItemQuantityReserved" ></property>
	<property name="inventoryItemQuantityOccupied" ></property>
	<property name="inventoryItemQuantityVirtual" ></property>
	<property name="inventoryItemCreatedBy" ></property>
	<property name="inventoryItemCreatedAt" ></property>
	<property name="inventoryItemUpdatedBy" ></property>
	<property name="inventoryItemUpdatedAt" ></property>

    <property name="createdBy" column="CreatedBy"/>

    <property name="createdAt" column="CreatedAt" />

    <property name="commandId" column="CommandId"/>

    <subclass name="AbstractInventoryItemEntryMvoStateEvent$SimpleInventoryItemEntryMvoStateCreated" discriminator-value="Created">
    	
    </subclass>

    <subclass name="AbstractInventoryItemEntryMvoStateEvent$SimpleInventoryItemEntryMvoStateMergePatched" discriminator-value="MergePatched">

        <property name="isPropertyQuantityOnHandVarRemoved" column="IsPropertyQuantityOnHandVarRemoved"/>

        <property name="isPropertyQuantityReservedVarRemoved" column="IsPropertyQuantityReservedVarRemoved"/>

        <property name="isPropertyQuantityOccupiedVarRemoved" column="IsPropertyQuantityOccupiedVarRemoved"/>

        <property name="isPropertyQuantityVirtualVarRemoved" column="IsPropertyQuantityVirtualVarRemoved"/>

        <property name="isPropertyEventVORemoved" column="IsPropertyEventVORemoved"/>

        <property name="isPropertyVersionRemoved" column="IsPropertyVersionRemoved"/>

        <property name="isPropertyActiveRemoved" column="IsPropertyActiveRemoved"/>

        <property name="isPropertyInventoryItemQuantityOnHandRemoved" column="IsPropertyInventoryItemQuantityOnHandRemoved"/>

        <property name="isPropertyInventoryItemQuantityReservedRemoved" column="IsPropertyInventoryItemQuantityReservedRemoved"/>

        <property name="isPropertyInventoryItemQuantityOccupiedRemoved" column="IsPropertyInventoryItemQuantityOccupiedRemoved"/>

        <property name="isPropertyInventoryItemQuantityVirtualRemoved" column="IsPropertyInventoryItemQuantityVirtualRemoved"/>

        <property name="isPropertyInventoryItemCreatedByRemoved" column="IsPropertyInventoryItemCreatedByRemoved"/>

        <property name="isPropertyInventoryItemCreatedAtRemoved" column="IsPropertyInventoryItemCreatedAtRemoved"/>

        <property name="isPropertyInventoryItemUpdatedByRemoved" column="IsPropertyInventoryItemUpdatedByRemoved"/>

        <property name="isPropertyInventoryItemUpdatedAtRemoved" column="IsPropertyInventoryItemUpdatedAtRemoved"/>


    </subclass>


  </class>
  
</hibernate-mapping>
