        <property name="isPropertyYearPlanUpdatedByRemoved" />

        <property name="isPropertyYearPlanVersionRemoved" />

        <property name="isPropertyYearPlanCreatedAtRemoved" />

        <property name="isPropertyYearPlanUpdatedAtRemoved" />

