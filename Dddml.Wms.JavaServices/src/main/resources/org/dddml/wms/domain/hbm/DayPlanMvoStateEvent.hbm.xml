		<key-property name="dayPlanIdYear" column="DayPlanIdYear" />
		<key-property name="dayPlanIdMonth" column="DayPlanIdMonth" />
		<key-property name="dayPlanIdDay" column="DayPlanIdDay" />
		<key-property name="personVersion" column="PersonVersion" />
	</composite-id>

    <discriminator column="StateEventType" type="string" />

	<property name="description" length="500"></property>

	<property name="version" ></property>

	<property name="active" ></property>

	<property name="monthPlanDescription" length="500"></property>

	<property name="monthPlanCreatedBy" ></property>

	<property name="monthPlanUpdatedBy" ></property>

	<property name="monthPlanVersion" ></property>

	<property name="monthPlanCreatedAt" ></property>

	<property name="monthPlanUpdatedAt" ></property>

	<property name="monthPlanActive" ></property>

	<property name="monthPlanDeleted" ></property>

	<property name="yearPlanDescription" length="500"></property>

	<property name="yearPlanCreatedBy" ></property>

	<property name="yearPlanUpdatedBy" ></property>

	<property name="yearPlanVersion" ></property>

	<property name="yearPlanCreatedAt" ></property>

	<property name="yearPlanUpdatedAt" ></property>

	<property name="yearPlanActive" ></property>

	<property name="yearPlanDeleted" ></property>

	<property name="personBirthDate" ></property>

	<component name="personLoves" class="org.dddml.wms.domain.PersonalName">
		<property name="firstName" column="personLovesFirstName" length="50"/>
		<property name="lastName" column="personLovesLastName" length="50"/>
	</component>

	<component name="personEmergencyContact" class="org.dddml.wms.domain.Contact">
		<property name="personalNameFirstName" column="personEmergencyContactPersonalNameFirstName" length="50"/>
		<property name="personalNameLastName" column="personEmergencyContactPersonalNameLastName" length="50"/>
		<property name="phoneNumber" column="personEmergencyContactPhoneNumber" />
		<property name="address" column="personEmergencyContactAddress" />
	</component>

	<property name="personEmail" ></property>

	<property name="personCreatedBy" ></property>

	<property name="personUpdatedBy" ></property>

	<property name="personCreatedAt" ></property>

	<property name="personUpdatedAt" ></property>

	<property name="personActive" ></property>

	<property name="personDeleted" ></property>


    <property name="createdBy" />

    <property name="createdAt" />

    <property name="commandId" />


    <subclass name="AbstractDayPlanMvoStateEvent$SimpleDayPlanMvoStateCreated" discriminator-value="Created">
    	
    </subclass>

    <subclass name="AbstractDayPlanMvoStateEvent$SimpleDayPlanMvoStateMergePatched"  discriminator-value="MergePatched">

        <property name="isPropertyDescriptionRemoved" />

        <property name="isPropertyVersionRemoved" />

        <property name="isPropertyActiveRemoved" />

        <property name="isPropertyMonthPlanDescriptionRemoved" />

        <property name="isPropertyMonthPlanCreatedByRemoved" />

        <property name="isPropertyMonthPlanUpdatedByRemoved" />

        <property name="isPropertyMonthPlanVersionRemoved" />

        <property name="isPropertyMonthPlanCreatedAtRemoved" />

